{
  "name": "@janeirodigital/interop-utils",
  "version": "1.0.0-rc.4",
  "description": "Internal utilities intended to be used by other modules implementing Solid Application Interoperability specification",
  "contributors": [
    {
      "name": "elf Pavlik",
      "email": "elf-pavlik@hackers4peace.net"
    },
    {
      "name": "Maciej Samoraj",
      "email": "maciej.samoraj@gmail.com"
    },
    {
      "name": "√Ångel Araya",
      "email": "angel.araya@janeirodigital.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/janeirodigital/sai-js.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:mjs && sh fixup.sh",
    "build:cjs": "tsc -b tsconfig-cjs.json",
    "build:mjs": "tsc -b tsconfig-mjs.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "clean": "rimraf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@shexjs/parser": "^1.0.0-alpha.15",
    "@shexjs/shape-path-query": "^1.0.0-alpha.18",
    "@shexjs/util": "^1.0.0-alpha.15",
    "http-link-header": "^1.0.3",
    "n3": "^1.11.1",
    "shape-path-core": "^0.0.3"
  },
  "devDependencies": {
    "@rdfjs/types": "^1.0.1",
    "@types/http-link-header": "^1.0.3",
    "@types/n3": "^1.10.0",
    "@types/shexj": "^2.1.1"
  }
}
