@prefix acl: <http://www.w3.org/ns/auth/acl#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

<#acme>
    a acl:Authorization;
    acl:agent <http://localhost:3000/kim/profile/card#me>;
    acl:accessTo <./>;
    acl:default <./>;
    acl:mode acl:Read.

<#owner>
    a acl:Authorization;
    acl:agent <http://localhost:3000/alice/profile/card#me>;
    acl:accessTo <./>;
    acl:default <./>;
    acl:mode
        acl:Read, acl:Write, acl:Control.
