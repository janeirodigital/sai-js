{
  "name": "@janeirodigital/interop-application",
  "version": "1.0.0-rc.18",
  "description": "Top-level module for applications confirming to Solid Application Interoperability specification",
  "contributors": [
    {
      "name": "elf Pavlik",
      "email": "elf-pavlik@hackers4peace.net"
    },
    {
      "name": "Maciej Samoraj",
      "email": "maciej.samoraj@gmail.com"
    },
    {
      "name": "√Ångel Araya",
      "email": "angel.araya@janeirodigital.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/janeirodigital/sai-js.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:mjs && sh fixup.sh",
    "build:cjs": "tsc -b tsconfig-cjs.json",
    "build:mjs": "tsc -b tsconfig-mjs.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "publish": "npm run clean && npm run build"
  },
  "dependencies": {
    "@janeirodigital/interop-data-model": "^1.0.0-rc.18",
    "@janeirodigital/interop-namespaces": "^1.0.0-rc.18",
    "@janeirodigital/interop-utils": "^1.0.0-rc.18",
    "n3": "^1.16.4"
  },
  "devDependencies": {
    "@janeirodigital/interop-test-utils": "^1.0.0-rc.18",
    "@rdfjs/types": "^1.1.0"
  }
}
